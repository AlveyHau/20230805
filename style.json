{
	"QCustomSlideMenu": [
        {
            "name": "leftMenuContainer",
            "defaultSize": [{
                "width": 45,
                "height": 530
            }],
            "collapsedSize": [{
                "width": 45,
                "height": 530
            }],
            "expandedSize": [{
                "width": 135,
                "height": 530
            }],
            "toggleButton": [{
                "buttonName": "menuBtn",
                "icons": [{
                    "whenMenuIsCollapsed": "./resources/menuExpandBtn1.png",
                    "whenMenuIsExpanded": "./resources/menuExpandBtn2.png"
                }]
            }],
            "menuTransitionAnimation": [{
                "animationDuration": 200,
                "animationEasingCurve": "Linear"
            }]
        },

        {
            "name": "centerMenuContainer",
            "defaultSize": [{
                "width": 0,
                "height": 530
            }],
            "collapsedSize": [{
                "width": 0,
                "height": 530
            }],
            "expandedSize": [{
                "width": 160,
                "height": 530
            }],
            "menuTransitionAnimation": [{
                "animationDuration": 200,
                "animationEasingCurve": "Linear"
            }]
        }
    ],


	"QStackedWidget":[
		{
			"name":"mainPages",
            "transitionAnimation":[
                {
                    "fade":[
                        {
                            "active": true,
                            "duration": 1000,
                            "easingCurve": "OutBack"
                        }
                    ],
                    "slide":[
                        {
                            "active": true,
                            "duration": 1000,
                            "direction": "horizontal",
                            "easingCurve": "OutBack"                            
                        }
                    ]
                }
            ],
			"navigation":[
				{
					"navigationButtons":[
						{
							"settingsBtn": "settingPage",
							"chartBtn": "chartPage",
                            "tableBtn": "tablePage"
						}
					]
				}
			]
		}      
	]
}
